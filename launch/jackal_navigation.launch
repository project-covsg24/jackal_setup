<launch>
  <!-- Arguments -->
  <arg name="map_file" default="$(find jackal_setup)/maps/3rd_floor.yaml"/>
  <arg name="move_forward_only" default="false"/>


  <!-- Map server -->
  <!-- <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/> -->

  <include file="$(find jackal_setup)/launch/urg_node_remappable.launch">
  </include>
  <!-- AMCL -->
  <include file="$(find jackal_setup)/launch/amcl_demo.launch"/>

  <!--- Run Move Base -->
 <include file="$(find jackal_setup)/launch/move_base.launch" />

  <!-- rviz -->
    <node pkg="rviz" type="rviz" name="rviz" required="true"
          args="-d $(find jackal_setup)/config/navigation.rviz"/>
</launch>
